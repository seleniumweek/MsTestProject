<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<RunConfiguration>
	</RunConfiguration>
	<LoggerRunSettings>
		<Loggers>
			<Logger friendlyName="console" enabled="True">
				<Configuration>
					<Verbosity>quiet</Verbosity>
				</Configuration>
			</Logger>
			<Logger friendlyName="trx" enabled="True">
				<Configuration>
					<LogFileName>C:\Users\Nares\source\repos\MsTestProject\MsTestProject\foo.trx</LogFileName>
				</Configuration>
			</Logger>
			<Logger friendlyName="html" enabled="True">
				<Configuration>
					<LogFileName>C:\Users\Nares\source\repos\MsTestProject\MsTestProject\foo.html</LogFileName>
				</Configuration>
			</Logger>
		</Loggers>
	</LoggerRunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<!-- Enables blame -->
			<DataCollector friendlyName="blame" enabled="True">
				<Configuration>
					<!-- Enables crash dump, with dump type "Full" or "Mini".
          Requires ProcDump in PATH for .NET Framework. -->
					<CollectDump DumpType="Full" />
					<!-- Enables hang dump or testhost and its child processes 
          when a test hangs for more than 10 minutes. 
          Dump type "Full", "Mini" or "None" (just kill the processes). -->
					<CollectDumpOnTestSessionHang TestTimeout="10min" HangDumpType="Full" />
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>